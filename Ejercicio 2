Practica 7 Ejercicio 2
#include <stdio.h>

int main()
{
    int n1, n2, n3;
    printf("Ingrese tres números separados por espacios.\n");
    scanf("%d %d %d", &n1, &n2, &n3);
    //Sección para el primer número.
    if (n1 > n2)
    {
        if (n1 < n3)
        {
            printf("El número central es %d.\n", n1);
        }
    }
    else
    {
        if (n1 > n3)
        {
            if (n1 < n2)
            {
                printf("El número central es %d.\n", n1);
            }
        }
    }
    
    //Sección para el segundo número.
    if (n2 > n1)
    {
        if (n2 < n3)
        {
            printf("El número central es %d.\n", n2);
        }
    }
    else
    {
        if (n2 > n3)
        {
            if (n2 < n1)
            {
                printf("El número central es %d.\n", n2);
            }
        }
    }
    //Sección para el tercer número.
    if (n3 > n2)
    {
        if (n3 < n1)
        {
            printf("El número central es %d.\n", n3);
        }
    }
    else
    {
        if (n3 > n1)
        {
            if (n3 < n2)
            {
                printf("El número central es %d.\n", n3);
            }
        }
    }

    return 0;
}
